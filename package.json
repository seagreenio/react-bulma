    "babel-loader": "^9.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^7.5.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.64.0",
    "tslib": "^2.4.1",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "typescript": "^4.8.4"
  },
